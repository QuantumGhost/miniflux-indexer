root = true

[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8

[*.py]
indent_style = space
indent_size = 4

[*.json]
indent_style = space
indent_size = 4

[*.js]
indent_style = space
indent_size = 4

[*.go]
indent_style = tab

[*.md]
trim_trailing_whitespace = false
